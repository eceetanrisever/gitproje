                <script type="text/javascript">
                    var login;
                    function harfUzunluk(id,masterDiv,limit){
                        var tut = $("#"+id).val().length;

                        if((limit - tut) <= 0){
                            var deger = $("#"+id).val(); $("#"+id).val(deger.slice(0,limit));
                            $("#"+masterDiv+" .count").html(  '<span></span>KARAKTER LİMİTİNİZ TÜKENDİ.' );
                            window.event.keyCode = 0; return;
                        }
                        $("#"+masterDiv+" .count").html( "<span>" + (limit - tut) + "</span> karakter kaldı" );
                    }

                    function yorum_gonder (hid) {
                        var yorum = $('.'+hid).val();
                        var isim = $('.i'+hid).val();
                        var avatar = $('.ya').val();
                        var resim = $('.im').val();
                        var video = $('.vd').val();
                        if($('#fbchk').prop('checked')){
                            var fbchk = $('#fbchk').val();
                        }else{
                            var fbchk = 0;
                        }
                        if($('#twchk').prop('checked')){
                            var twchk = $('#twchk').val();
                        }else{
                            var twchk = 0;
                        }

                        var url = 'yilmaz-ozdilin-ataturk-uzerinden-cikardigi-kitap-serisi';

                        if(!document.getElementById('ksozlesmeyorum').checked) {
                            alert('Yorum gönderebilmek için yorum kurallarını kabul etmeniz gerekiyor.');
                            return
                        }

                        if(yorum != '' && isim != ''){

                            if (!$('.yorumGonder').hasClass('disabled')){
                                $(".yorumGonder").addClass('disabled');

                                $.post('/esh/sayfalar/yorum_gonder.php',{hid: 574355, isim: isim, yorum: yorum, avatar:avatar,video:video,resim:resim, fbchk:fbchk, twchk:twchk, url:url}, "html").success(function(data){ $('#addComment').html(data); });

                            }

                        }
                    };

                    function cevap_gonder (cvp) {

                        if(!document.getElementById('ksozlesmecevap').checked) {
                            alert('Yorum\'a cevap gönderebilmek için gönderebilmek için yorum kurallarını kabul etmeniz gerekiyor.');
                            return
                        }


                        var hid = $(cvp).parent().parent().parent().attr('id');
                        var cevap = $('#replyComment #commentInput').val();
                        var isim = $('#replyComment .name input').val();

                        if(cevap != '' && isim != ''){
                            $('#replyComment').slideUp();
                            alert('Cevabınız gönderildi. Onaylandıktan sonra yayınlanacaktır.');
                            $.post('/esh/sayfalar/yorum_gonder.php', {hid: hid, isim:isim, yorum: cevap, cevap: 1}, function (cevap) {
                                if(cevap == 'tamam') {
                                    $('#replyComment .name input').val('');
                                    $('#replyComment #commentInput').val('');
                                }
                            });
                        }
                    }

                    function anchorRF(){
                        this.run = function(){
                            $('a[href="#"]').click(function(){
                                return false;
                            });
                        }
                    }
                    var rf = new anchorRF();

                    
                    function yorumlari_goster(haberID,tipID) {
                        $.post('/esh/sayfalar/yorumlar_v2.1.php', {id: haberID,tip:tipID}, function (sonuc) {
                            $('.commentMaster'+haberID).html(sonuc);
                            commentsItemInit();
                        });
                    }

                    
                    function begen(id, begen, cevap){
                        $('.like'+id).hide();
                        $('.dislike'+id).attr('onclick','').html('Oyunuz kullanıldı.');
                        if(cevap) {
                            $.post('/esh/sayfalar/yorumlar_v2.1.php', {id: id, cevap: 1, begen: begen});
                        }else{
                            $.post('/esh/sayfalar/yorumlar_v2.1.php', {id: id, begen: begen});
                        }
                    }


                    function commentsItemInit(){

                        $('#masterComment').click(function(){
                            if ($('.hiddenContent').height()<=30){
                                $('#addComment .hiddenContent').animate({'height':'73px'},250);
                            }
                        });

                        $(' .reply').click(function(){
                            $(this).parent().parent().append($('#replyComment'));
                            $('#replyComment').slideDown('fast');
                            $('#replyComment .cancel').click(function(){
                                $('#replyComment').slideUp('fast');
                            });
                            clearInterval(autoRefresh);
                            rf.run();
                        });

                        if (login == 1) {
                            userloginmode(1);
                        }else{
                            userloginmode(0);
                        }

                    }

